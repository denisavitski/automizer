      "allowedExtensions": [
        "lib/allowedExtensions.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsx ./playground/index.ts",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/node": "^22.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.2"
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "favicons": "^7.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "node-html-parser": "^6.1.13",
    "sharp": "^0.33.4",
    "urlpattern-polyfill": "^10.0.0"
  },
  "peerDependencies": {
    "dedent": "^1.5.3",
    "favicons": "^7.2.0",
    "fluent-ffmpeg": "^2.1.3",
