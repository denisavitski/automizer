{
  "name": "automizer",
  "version": "0.3.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denisavitski/automizer.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/denisavitski/automizer/issues"
  },
  "homepage": "https://github.com/denisavitski/automizer#readme",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./allowedExtensions": {
      "import": "./allowedExtensions.js",
      "types": "./allowedExtensions.d.ts"
    }
  },
  "typesVersions": {
